<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="TFG_Projects_APP_Frontend.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:TFG_Projects_APP_Frontend"
    xmlns:pages="clr-namespace:TFG_Projects_APP_Frontend.Pages"
    Shell.FlyoutBehavior="Flyout"
    Title="Project Design APP">

    <ShellContent
    Title="Log Out"
    ContentTemplate="{DataTemplate pages:LoginPage}"
    Route="LoginPage" />

    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate pages:MainPage}"
        Route="MainPage" />
</Shell>
